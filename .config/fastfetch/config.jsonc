{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
    "logo": {
        "type": "small",
        "padding": {
            "top": 2
        }
    },
    "display": {
        "separator": "  "
    },
    "modules": [
        "break",
        "title",
        {
            "type": "os",
            "key": "os    ",
            "keyColor": "red"
        },
        {
            "type": "kernel",
            "key": "kernel",
            "keyColor": "green"
        },
        {
            "type": "host",
            "format": "{vendor} {family}",
            "key": "host  ",
            "keyColor": "yellow"
        },
        {
            "type": "packages",
            "key": "pkgs  ",
            "keyColor": "blue"
        },
        {
            "type": "uptime",
            "format": "{?days}{days}d {hours}h {minutes}m",
            "key": "uptime",
            "keyColor": "magenta"
        },
        {
            "type": "memory",
            "key": "memory",
            "keyColor": "cyan"
        },
        {
            "type": "battery",
            "key": "bat   ",
            "keyColor": "yellow"
        },
        "break"
    ]
}
